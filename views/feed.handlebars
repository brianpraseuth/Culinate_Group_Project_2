	<div class="jumbotron">
	    <div class="searchForm">
	        <h1 class="nav-brand">the Feed</h1>
	                <a href="/">Search Recipes</a>
		<a href="/feed">Saved Recipes</a>
	        <hr class="my-4">
            <h2 class="grand-hotel second-heading">Saved Recipes</h2>
	    </div>
	</div>

	<div class="container">
	    <div id="recipeCards">
            <div class="row">
	        {{#each recipe}}
	        <div class="recipe-card">
	            <h3 class="text-center">{{this.name}}</h3>
	            <img class="entreeImage" src={{this.recipeImage}}>
	            <p>{{this.instructions}}</p>
                <button class="btn-light delete-recipe" data-id="{{this.id}}">DELETE!</button>
	        </div>
	        {{/each}}
            </div>
	    </div>
	</div>
	</div>